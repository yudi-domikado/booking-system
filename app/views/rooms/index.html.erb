<h1>Rooms</h1>
<table class="table">
<thead>
<tr>
	<th>Picture</th>
	<th>Name</th>
	<th>Description</th>
	<th>Price</th>
	<th>Booking?</th>
</tr>
</thead>
<tbody>
<% @rooms.each do |kamar| %>
  <tr>
  	<td><%= image_tag kamar.picture.url(:thumb) %></td>
    <td><%= kamar.name %></td>
    <td><%= kamar.description %></td>
    <td><%= kamar.price %></td>
    <td><%= link_to "Booking", room_path(kamar) %></td>
  </tr>
<% end %>
</tbody>
</table>